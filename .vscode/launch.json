{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch BoosterServer (Debug Verbose)",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/BoosterServer/dist/server.js",
      "preLaunchTask": "npm: compile",
      "outFiles": [
        "${workspaceFolder}/BoosterServer/dist/**/*.js"
      ],
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "debug"
      },
      "sourceMaps": true,
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch BoosterServer (Info Logging)",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/BoosterServer/dist/server.js",
      "preLaunchTask": "npm: compile",
      "outFiles": [
        "${workspaceFolder}/BoosterServer/dist/**/*.js"
      ],
      "env": {
        "LOG_LEVEL": "info"
      },
      "sourceMaps": true,
      "console": "integratedTerminal"
    }
  ]
} 