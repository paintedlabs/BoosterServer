{
  "name": "express-test",
  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
    "prepare": "ts-patch install",
    "start": "NODE_OPTIONS='--max-old-space-size=12288' ts-node -r tsconfig-paths/register server.ts",
    "dev": "NODE_OPTIONS='--max-old-space-size=12288' ts-node-dev --respawn -r tsconfig-paths/register --compiler-options '{\"plugins\":[{\"transform\":\"typia/lib/transform\"}]}' server.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\""
  },
  "dependencies": {
    "@apollo/server": "^4.12.0",
    "@graphql-tools/utils": "^10.8.6",
    "@samchon/openapi": "^4.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "graphql-tools": "^9.0.18",
    "node-fetch": "^2.6.9",
    "sharp": "^0.33.5",
    "stream-json": "^1.7.5",
    "typia": "^9.3.0",
    "unzipper": "^0.12.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "@types/unzipper": "^0.10.11",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.1",
    "eslint-plugin-prettier": "^5.2.6",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-module": "^0.2.1",
    "ts-essentials": "^10.0.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^3.3.0",
    "tsconfig-paths": "^4.2.0",
    "typed-emitter": "^2.1.0",
    "typescript": "~5.8.3",
    "web-streams-polyfill": "^4.1.0"
  }
}
